#include <iostream>
using namespace std;

class Order
{
    string MinCharge;
    int TableNum;
    string ServName;
    int HowMany;
    public:
    void setMinCharge(string);
    void setTableNum(int);
    void setServName(string);
    void setHowMany(int);
    void displayOrder();
};

void Order::displayOrder()
{
    cout << MinCharge;
}

void Order::setMinCharge(string min)
{
    MinCharge = min;
}

int main()
{
    Order aOrder;
    aOrder.setMinCharge("$4.75");
    aOrder.displayOrder();

    return 0;
}
